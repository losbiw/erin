{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "es5",
    "module": "commonjs",
    "lib": ["esnext", "dom"],
    "jsx": "react",
    "sourceMap": true,
    "outDir": "../build/",
    "removeComments": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,    
  }
}