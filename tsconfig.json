{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "jsx": "react",
    "sourceMap": true,
    "outDir": "./build/",
    "removeComments": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@/*": [
        "src/components/*"
      ],
      "@icons/*": [
        "src/assets/Icons/*"
      ],
      "@nav/*": [
        "src/assets/nav/*"
      ],
      "@info/*": [
        "src/assets/info/*"
      ],
      "@ui/*": [
        "src/assets/ui/*"
      ],
      "@themes/*": [
        "src/assets/themes/*"
      ],
      "@controls/*": [
        "src/assets/controls/*"
      ],
      "@settings/*": [
        "src/assets/settings/*"
      ],
      "@logo/*": [
        "src/assets/app/*"
      ],
      "@modules/*": [
        "src/modules/*"
      ],
      "@interfaces/*": [
        "src/interfaces/*"
      ],
      "@helpers/*": [
        "src/helpers/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@redux/*": [
        "src/redux/*"
      ],
      "@slices/*": [
        "src/redux/slices/*"
      ],
      "@app/*": [
        "src/app/*"
      ]
    }
  }
}