{
  "name": "erin",
  "version": "0.1.0",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "auto-launch": "^5.0.5",
    "dotenv": "^8.2.0",
    "node-fetch": "^2.6.1",
    "raw-loader": "^4.0.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-scripts": "3.4.4"
  },
  "main": "src/electron/main.js",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "electron": "electron ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config-overrides-path": "src/config/config-override.js",
  "devDependencies": {
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "electron-debug": "^3.1.0",
    "electron-devtools-installer": "^3.1.1",
    "react-app-rewired": "^2.1.6"
  },
  "build": {
    "extraResources": [
      "**/build/**"
    ],
    "appId": "com.erin.start",
    "productName": "Erin",
    "win": {
      "icon": "build/icon.ico",
      "target": "nsis"
    },
    "mac": {
      "icon": "build/icon.512x512.png",
      "target": "dmg"
    },
    "linux": {
      "icon": "build/icon.512x512.png",
      "target": [
        "deb",
        "rpm",
        "snap"
      ],
      "category": "Utility"
    }
  }
}
